package com.mralexan.sapphiremod.blocks;

import com.mralexan.sapphiremod.Sapphire;

import net.minecraft.block.material.Material;

public class SapphireBlocks
{
	public static void initializeBlocks()
	{
		Sapphire.sapphireBlock = new SapphireBlock(Material.rock, "sapphireBlock", 1.5F);
		Sapphire.sapphireOre = new SapphireOre(Material.rock, "sapphireOre", 1.5F);
	}
}
