package com.mralexan.sapphiremod;

import net.minecraft.item.ItemStack;
import cpw.mods.fml.common.registry.GameRegistry;

public class RecipesRegister
{
	public void register()
	{
		ItemStack sapphire = new ItemStack(Sapphire.sapphireItem, 9);
		ItemStack sapphireBlock = new ItemStack(Sapphire.sapphireBlock);
		
		GameRegistry.addShapelessRecipe(sapphire, sapphireBlock);
		
	}
}
