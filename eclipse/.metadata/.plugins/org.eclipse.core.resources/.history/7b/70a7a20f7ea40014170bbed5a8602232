package com.mralexan.sapphiremod.registers;

import com.mralexan.sapphiremod.Sapphire;

import cpw.mods.fml.common.registry.GameRegistry;

public class BlocksRegister
{
	public static void registerBlocks()
	{
    	GameRegistry.registerBlock(Sapphire.sapphireBlock, "sapphireBlock");
    	GameRegistry.registerBlock(Sapphire.sapphireOre, "sapphireOre");
	}
}
